<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: "Jack Corley's Portfolio", sticky: false) %>
  <% elsif flash[:error] %>
    <%= js add_gritter(flash[:error], title: "Jack Corley's Portfolio", sticky: false) %>
<% elsif flash[:notice] %>
  <%= js add_gritter(flash[:notice], title: "Jack Corley's Portfolio", sticky: false) %>
<% end %>